{
    "name": "jeidison/pdf-signer",
    "description": "Project to add digital signature in pdf files",
    "type": "library",
    "require": {
        "ext-openssl": "*",
        "ext-zlib": "*",
        "ext-fileinfo": "*",
        "ramsey/uuid": "^4.7"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jeidison\\PdfSigner\\": "src/",
            "Tests\\": "tests"
        },
        "files": [
            "src/Utils/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Jeidison Farias"
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "bamarni/composer-bin-plugin": true
        },
        "platform": {
            "php": "8.0"
        }
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "dev-master"
    }
}
